{
  "name": "todo-app",
  "version": "1.0.0",
  "description": "ToDo App",
  "private": true,
  "scripts": {
    "start": "webpack-dev-server -d --hot --inline --no-info --port 3000",
    "test": "npm-run-all --silent test-*",
    "test-lint": "./node_modules/.bin/eslint ./*.js find ./config ./controllers ./routes ./services ./src ./test '*.js'",
    "test-mocha": "./node_modules/.bin/nyc ./node_modules/.bin/mocha --require jsdom-global --recursive --reporter mochawesome --reporter-options reportDir=./coverage ./test/test-setup.js ./test --exit"
  },
  "dependencies": {
    "apollo-client": "^0.4.1",
    "classnames": "2.2.5",
    "graphql-tag": "^0.1.9",
    "react": "^15.2.1",
    "react-apollo": "^0.3.19",
    "react-dom": "^15.2.1",
    "redux": "^3.5.2",
    "whatwg-fetch": "^1.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "6.5.0",
    "babel-register": "^6.26.0",
    "css-loader": "^0.23.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.10.0",
    "eslint": "^3.0.1",
    "eslint-config-standard": "^5.3.5",
    "eslint-config-standard-react": "^3.0.0",
    "eslint-loader": "^1.4.1",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-promise": "^2.0.0",
    "eslint-plugin-react": "^5.2.2",
    "eslint-plugin-standard": "^2.0.0",
    "html-webpack-plugin": "^2.22.0",
    "jsdom": "^16.5.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^6.0.2",
    "mochawesome": "^3.1.1",
    "npm-run-all": "^4.1.5",
    "nyc": "^13.3.0",
    "style-loader": "^0.13.1",
    "webpack": "1.13.1",
    "webpack-dev-server": "1.14.1"
  }
}
